{
	"name": "@jdeighan/coffee-utils",
	"type": "module",
	"version": "16.0.21",
	"description": "A set of utility functions for CoffeeScript",
	"main": "coffee_utils.js",
	"exports": {
		".": "./src/lib/browser.js",
		"./server": "./src/lib/server.js",
		"./browser": "./src/lib/browser.js",
		"./fs": "./src/lib/fs.js",
		"./log": "./src/lib/log.js",
		"./block": "./src/lib/block.js",
		"./indent": "./src/lib/indent.js",
		"./html": "./src/lib/html.js",
		"./stack": "./src/lib/stack.js",
		"./taml": "./src/lib/taml.js",
		"./section": "./src/lib/Section.js",
		"./sectionmap": "./src/lib/SectionMap.js",
		"./fsa": "./src/lib/fsa.js",
		"./giftset": "./src/lib/GiftSet.js",
		"./keyedset": "./src/lib/KeyedSet.js",
		"./package.json": "./package.json"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"ava": {
		"verbose": false
	},
	"scripts": {
		"build": "cls && coffee -c .",
		"test": "npm run build && npx ava ./test/*.test.js && git status"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/johndeighan/coffee-utils.git"
	},
	"keywords": [
		"coffeescript"
