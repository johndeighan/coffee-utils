{
	"name": "@jdeighan/coffee-utils",
	"type": "module",
	"version": "2.1.3",
	"description": "A set of utility functions for CoffeeScript",
	"main": "coffee_utils.js",
	"exports": {
		".": "./src/coffee_utils.js",
		"./fs": "./src/fs_utils.js",
		"./log": "./src/log_utils.js",
		"./block": "./src/block_utils.js",
		"./indent": "./src/indent_utils.js",
		"./debug": "./src/debug_utils.js",
		"./svelte": "./src/svelte_utils.js",
		"./test": "./src/UnitTester.js",
		"./package.json": "./package.json"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"scripts": {
		"build": "cls && coffee -c .",
		"pretest": "cls && coffee -c .",
		"test": "ava ./test/*.test.js",
		"prefinaltest": "cls && coffee -c .",
		"finaltest": "cross-env FINALTEST=yes ava ./test/*.test.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/johndeighan/coffee-utils.git"
	},
	"keywords": [
		"coffeescript"
	],
	"author": "John Deighan",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/johndeighan/coffee-utils/issues"
	},
	"homepage": "https://github.com/johndeighan/coffee-utils#readme",
	"dependencies": {
		"ava": "^3.15.0",
		"cross-env": "^7.0.3",
		"js-yaml": "^4.1.0"
	}
}
