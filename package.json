	"author": "John Deighan",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/johndeighan/coffee-utils/issues"
	},
	"homepage": "https://github.com/johndeighan/coffee-utils#readme",
	"dependencies": {
		"@jdeighan/base-utils": "^8.0.23",
		"cross-env": "^7.0.3",
		"n-readlines": "^1.0.1",
		"readline-sync": "^1.4.10",
		"svelte": "^4.1.1"
	},
	"devDependencies": {
		"@jdeighan/unit-tester": "^3.0.70",
		"ava": "^6.0.1"
	}
}
